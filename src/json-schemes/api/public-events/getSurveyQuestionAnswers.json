{
  "schema": "http://json-schema.org/draft-07/schema#",
  "title": "Получение ответов на вопрос анкеты",
  "type": "object",
  "additionalProperties": true,
  "required": ["surveyId", "questionId"],
  "properties": {
    "surveyId": {
      "description": "ID анкеты",
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    },
    "questionId": {
      "description": "ID вопроса",
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    },
    "eventId": {
      "description": "Опц. ID мероприятия",
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    }
  }
}