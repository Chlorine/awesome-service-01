{
  "schema": "http://json-schema.org/draft-07/schema#",
  "title": "Получение информации о мероприятиях с некоторой анкетой",
  "type": "object",
  "additionalProperties": true,
  "required": ["surveyId"],
  "properties": {
    "surveyId": {
      "description": "ID анкеты",
      "type": "string",
      "pattern": "^[0-9a-fA-F]{24}$"
    }
  }
}